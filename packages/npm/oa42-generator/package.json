{
  "name": "oa42-generator",
  "version": "0.9.20",
  "sideEffects": false,
  "description": "",
  "type": "module",
  "main": "./bundled/main.cjs",
  "module": "./bundled/main.js",
  "types": "./typed/main.d.ts",
  "exports": {
    ".": {
      "require": "./bundled/main.cjs",
      "import": "./bundled/main.js",
      "types": "./typed/main.d.ts"
    }
  },
  "files": [
    "./typed/**",
    "./bundled/**"
  ],
  "bin": {
    "oa42-generator": "bundled/program.js"
  },
  "scripts": {
    "prepack": "node ./scripts/build.js",
    "pretest": "tsc",
    "build": "node ./scripts/build.js",
    "clean": "node ./scripts/clean.js",
    "test": "node --test ./transpiled/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/OpenApi42"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/OpenApi42/issues"
  },
  "keywords": [
    "openapi",
    "generator"
  ],
  "dependencies": {
    "@jns42/oas-v3-0": "^0.17.4",
    "@jns42/oas-v3-1": "^0.17.4",
    "@jns42/swagger-v2": "^0.17.4",
    "@oa42/core": "^0.1.0",
    "@types/node": "^20.12.8",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "goodrouter": "^2.1.6",
    "jns42-generator": "^0.17.5",
    "oa42-lib": "^0.9.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.18.1",
    "yaml": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.5",
    "@tsconfig/node20": "^20.1.4",
    "rollup": "^4.17.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  }
}
